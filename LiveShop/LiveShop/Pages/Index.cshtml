@page
@model IndexModel
@{
    ViewData["Title"] = "Home page";
}

<div class="container">
    <div class="col-md-4">
        <div class="x_panel">
            <div class="x_title">
                <h4>Products</h4>
                <div class="clearfix"></div>
            </div>
            <div class="x_content">
                <table class="table table-hover" id="tblProduct">
                    <thead>
                        <tr>
                            <th>#</th>
                            <th>Name</th>
                            <th>Manufacturer</th>
                            <th>Price</th>
                            <th>Amount</th>
                            <th></th>
                        </tr>
                    </thead>
                    <tbody></tbody>
                </table>
            </div>
        </div>
    </div>
</div>

@section Scripts{
    <script src="~/lib/microsoft/signalr/dist/browser/signalr.js"></script>
    <script src="~/js/dashboard.js"></script>
}
